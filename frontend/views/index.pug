extends layout

block content
            .w3-card-4
                header.w3-container.w3-teal
                    h1 Programação Canais de Filmes e Séries
            .w3-container.w3-padding
                table.w3-table-all
                    tr
                        th Dia
                        th Canal
                        th Programa
                        th Hora
                        th Rating
                    each canal,key in programacao
                        each programa in canal             
                            tr
                                td= programa.dia
                                td= key
                                td= programa.programa
                                td= programa.hora
                                td
                                    a(href="/rating/" + programa.programa)= "[*]"
            
